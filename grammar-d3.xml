<grammar>
  <item name="D3/Script" type="d3.script" new="D3 Visualisation">  
    <group name="Data" repeat="*" class="box-body">
      <arg type="d3.cmd" name="command"/>
      <!--<arg type="number|range" name="command"/>-->
    </group>    
  </item>

  <item name="D3/Data" type="d3.cmd.select">
    Data: <arg type="text|file" name="filename"/>
    <group name="Data" repeat="*" class="box-body">
      <arg type="d3.data" name="command"/>
    </group>
  </item>
  
  <item name="Range" type="range">
    <arg type="number" name="start"/> â†’ <arg type="number" name="end"/>
  </item>
  
  <item name="D3/Filter/Row" type="d3.data.keeprow">
    Keep Row: <arg type="number|range" name="rows"/>
  </item>
  
  <item name="D3/Filter/Column" type="d3.data.keepcolumn">
    Keep Column: <arg type="number|range" name="columns"/>
  </item>  
  
  <item name="D3/Filter/RemoveRow" type="d3.data.removerow">
    Remove Rows: <arg type="number|range" name="rows"/>
  </item>
  
  <item name="D3/Filter/RemoveColumn" type="d3.data.removecolumn">
    Remove Columns: <arg type="number|range" name="columns"/>
  </item>  
  
  <item name="D3/Transpose" type="d3.data.transpose">
    transpose
  </item>  

  <item name="D3/Append" type="d3.plot.append">
    .append(<arg type="svg.shape" name="Appendtag"/>)
  </item>

  <item name="D3/Attr" type="d3.plot.attr">
    .attr(<arg type="text" name="attrib"/>, <arg type="text" name="value"/>)
  </item>
  
  <item name="SVG/Shape/Circle" type="svg.shape.circle">
    circle
  </item>

  <item name="SVG/Shape/Rect" type="svg.shape.rect">
    rect
  </item>
 
</grammar>
